instance Block b, d, c, a;
[start] clear(a) := true;
[start] clear(c) := true;
[start] clear(d) := true;
[start] on(a) := TABLE;
[start] on(b) := TABLE;
[start] on(d) := TABLE;
[start] on(c) := b;
