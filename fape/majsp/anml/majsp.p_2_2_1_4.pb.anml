// instance2_4_1

instance Robot r0, r1;
instance Position p0, p1, p2, p3;
instance Treatment t0, t1;
instance Pallet b0;


[start] {
    	r0.at := p3;
	r1.at := p3;


    	r0.free := true;
	r1.free := true;


    	b0.at := p3;

    
    	p0.free := true;
	p1.free := true;
	p2.free := true;
	p3.free := true;

};

	p3.is_depot := true;


p0.can_do(t0) := true;
p1.can_do(t1) := true;



[all] contains unordered(
  process(b0, t0),
  process(b0, t1)  
);


