// instance1_4_3

instance Hoist h0;
instance Position p0, p1, p2, p3;
instance Bar b0, b1, b2;

h0.can_go(p0) := true;
h0.can_go(p1) := true;
h0.can_go(p2) := true;
h0.can_go(p3) := true;



[start] {
	b0.at := void;
	b1.at := void;
	b2.at := void;

    
	h0.at := p0;
    

	h0.free := true;


	p0.free := true;
	p1.free := true;
	p2.free := true;
	p3.free := true;
    

	plant_loading_position := p0;
	plant_unloading_position := p3;
};

